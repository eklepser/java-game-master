<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.menus.room_selection.PasswordConfirmationController"
      prefHeight="400.0" prefWidth="600.0"
      style="-fx-background-color: rgba(0, 200, 0, 0.5);">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Enter the password:"/>
    <PasswordField fx:id="passwordField"/>
    <HBox>
        <Button fx:id="backButton" onAction="#onBackButton" text="Back" prefWidth="100.0"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button fx:id="submitButton" onAction="#onSubmitButton" text="Enter" prefWidth="100.0"/>
    </HBox>
</VBox >
