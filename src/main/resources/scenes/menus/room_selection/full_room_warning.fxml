<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.menus.room_selection.FullRoomWarningController"
      style="-fx-background-color: rgba(0, 0, 0, 0.16);
             -fx-focus-color: #5a5a5a;
             -fx-faint-focus-color: transparent;"
      onKeyReleased="#onKeyReleased"
      alignment="CENTER">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Selected room is Full"/>
    <Region prefHeight="20"/>
    <Button fx:id="okButton" text="OK" onAction="#onOkButton"/>

</VBox>
